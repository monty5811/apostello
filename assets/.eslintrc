{
  "root": true,
  "extends": "airbnb",
    "rules": {
      "global-require": [0],
      "new-cap": [0],
      "no-unused-vars": [1],
      "react/forbid-prop-types": [0],
      "jsx-a11y/no-static-element-interactions": [0],
      "class-methods-use-this": [0],
      "import/no-dynamic-require": [0],
      "import/no-extraneous-dependencies": [0],
	  "import/no-unresolved": [2, { ignore: ['react$', 'react-dom$'] }],
      "import/extensions": [0],
    },
  "env": {
      "browser": true
    }
}
